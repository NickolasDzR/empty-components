{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Node 16",

  "compilerOptions": {
    "lib": ["ES6", "esnext", "dom", "dom.iterable"],
    "target": "ESNext",
    "noImplicitAny": false,
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": ".",
    "paths": {
      "@splideJs": ["node_modules/@splidejs/splide/dist/js/splide.esm.js"],
      "@splideCss": ["node_modules/@splidejs/splide/dist/css/splide-core.min.css"],
    },
    "module": "esnext",
    "types": ["vite/client"]
  },
  "typeRoots": [
    "types",
    "node_modules/@types"
  ],
  "skipLibCheck": true,
  "include": ["vite.config.js", "resources"],
  "allowUnreachableCode": false
}
